  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>CMMonitor</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- remove the total inserted from the selection
DECLARE @ExcludeDate AS DATETIME  
SELECT @ExcludeDate = MIN(InsertedDate) FROM CMMonitor.dbo.SystemDiscoveryArchive

SELECT CONVERT(VARCHAR(15),DeletedDate, 101) AS ActivityDate,
	COUNT(SystemDiscoveryArchivePK) AS DeletedTotal, NULL AS InsertedTotal
FROM CMMonitor.dbo.SystemDiscoveryArchive
WHERE DeletedDate IS NOT NULL
	AND CONVERT(VARCHAR(15), DeletedDate, 101) BETWEEN @FromDate AND @ToDate
GROUP BY CONVERT(VARCHAR(15),DeletedDate, 101)

UNION
 
SELECT CONVERT(VARCHAR(15),InsertedDate, 101) AS ActivityDate,
	NULL AS DeletedTotal, COUNT(SystemDiscoveryArchivePK) AS InsertedTotal
FROM CMMonitor.dbo.SystemDiscoveryArchive
WHERE InsertedDate IS NOT NULL
	AND CONVERT(VARCHAR(15), InsertedDate, 101) BETWEEN @FromDate AND @ToDate
	-- AND CONVERT(VARCHAR(15), InsertedDate, 101) &gt; @ExcludeDate
GROUP BY CONVERT(VARCHAR(15),InsertedDate, 101)  </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ActivityDate">
          <DataField>ActivityDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DeletedTotal">
          <DataField>DeletedTotal</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="InsertedTotal">
          <DataField>InsertedTotal</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
